  "compilerOptions": {
    "target": "esnext",
    "module": "node16",
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node16",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
